<?xml version="1.0" encoding="UTF-8"?>
<config>
  <!-- Uncomment this if you have PEAR Log installed
  <log>
    <type>file</type>
    <name>/path/to/propel.log</name>
    <ident>propel-bookstore</ident>
    <level>7</level>
  </log>
  -->
  <propel>
    <datasources default="limbo3">
      <datasource id="limbo3">
        <adapter>sqlite</adapter> <!-- sqlite, mysql, myssql, oracle, or pgsql -->
        <connection>
          <dsn>sqlite:/var/www/limbo3/limbo3</dsn>
        </connection>
      </datasource>
    </datasources>
  </propel>
</config>
